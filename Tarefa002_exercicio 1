#include <stdio.h>

int main() {
    int numVisitantes;

    // Solicita ao recepcionista o número de visitantes
    printf("Digite o número de visitantes que entraram na empresa hoje: ");
    scanf("%d", &numVisitantes);

    // Exibe a confirmação
    printf("Número de visitantes registrado com sucesso! Hoje, %d visitantes entraram na empresa.\n", numVisitantes);

    return 0;
}
